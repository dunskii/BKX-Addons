includes:
    - vendor/szepeviktor/phpstan-wordpress/extension.neon

parameters:
    level: 5
    paths:
        - src
    bootstrapFiles:
        - vendor/php-stubs/wordpress-stubs/wordpress-stubs.php
    ignoreErrors:
        - '#Function apply_filters invoked with \d+ parameters, 2 required#'
        - '#Function do_action invoked with \d+ parameters, 1 required#'
        - '#Constant BKX_\w+ not found#'
        - '#Access to undefined constant BKX_\w+#'
